{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": false,
		"moduleResolution": "node",
		"noEmit": false,
		"composite": false,
		"module": "ESNext",
		"target": "ES2020",
		"lib": ["ES2020"],
		"rootDir": "src/utils"
	},
	"include": ["src/utils/**/*.ts"]
}
