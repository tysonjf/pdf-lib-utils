{
	"name": "@tysonjf/pdf-lib-utils",
	"author": "Tyson JF",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tysonjf/pdf-lib-utils"
	},
	"private": false,
	"version": "0.1.1",
	"description": "A collection of utilities for drawing text and shapes in PDF documents using pdf-lib.",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"keywords": [
		"pdf",
		"pdf-lib",
		"pdf-lib-utils",
		"pdf-lib-utils-cjs",
		"pdf-lib-utils-esm"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build:lib": "tsup src/utils/index.ts --dts --format esm,cjs --out-dir dist --external @cantoo/pdf-lib --tsconfig tsconfig.lib.json",
		"prepublishOnly": "pnpm run build:lib",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview"
	},
	"peerDependencies": {
		"@cantoo/pdf-lib": "^2.4.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.25.0",
		"@pdf-lib/fontkit": "^1.1.1",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"eslint": "^9.25.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"pdfjs-dist": "^5.3.31",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tsup": "^8.5.0",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@types/qrcode": "^1.5.5",
		"qrcode": "^1.5.4"
	}
}
